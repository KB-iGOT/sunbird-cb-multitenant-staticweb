    "@types/jasmine": "~4.3.0",
    "@types/jest": "29.5.13",
    "@types/zip.js": "2.0.28",
    "jasmine-core": "~4.6.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "14.2.4",
    "jest-localstorage-mock": "^2.4.26",
    "jest-sonar-reporter": "^2.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
